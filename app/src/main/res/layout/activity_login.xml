<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".LoginActivity">

    <View
        android:id="@+id/rectangle_1"
        android:layout_width="286dp"
        android:layout_height="441dp"
        android:layout_marginTop="218dp"
        android:layout_marginBottom="255dp"
        android:background="@drawable/rectangle"
        android:layout_marginStart="63dp"
        android:layout_marginEnd="63dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.552"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <View
        android:id="@+id/rectangle_2"
        android:layout_width="247dp"
        android:layout_height="44dp"
        android:layout_alignParentTop="true"
        android:background="@drawable/rectangle1"
        android:layout_marginTop="167dp"
        android:layout_marginBottom="230dp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_1"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_1"
        app:layout_constraintStart_toStartOf="@+id/rectangle_1"
        app:layout_constraintTop_toTopOf="@+id/rectangle_1" />

    <View
        android:id="@+id/rectangle_3"
        android:layout_width="247dp"
        android:layout_height="44dp"
        android:layout_alignParentTop="true"
        android:background="@drawable/rectangle1"
        android:layout_marginTop="242dp"
        android:layout_marginBottom="155dp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_1"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_1"
        app:layout_constraintStart_toStartOf="@+id/rectangle_1"
        app:layout_constraintTop_toTopOf="@+id/rectangle_1" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="31dp"
        android:layout_marginTop="37dp"
        app:layout_constraintBottom_toTopOf="@+id/rectangle_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo" />

    <TextView
        android:id="@+id/welcome"
        android:layout_width="104dp"
        android:layout_height="30dp"
        android:layout_alignParentTop="true"
        android:gravity="top"
        android:text="@string/welcome"
        android:textAppearance="@style/welcome"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="401dp"
        android:layout_marginStart="92dp"
        android:layout_marginEnd="92dp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_1"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_1"
        app:layout_constraintStart_toStartOf="@+id/rectangle_1"
        app:layout_constraintTop_toTopOf="@+id/rectangle_1" />

    <TextView
        android:id="@+id/username"
        android:layout_width="83dp"
        android:layout_height="24dp"
        android:layout_alignParentTop="true"
        android:gravity="top"
        android:text="@string/username"
        android:textAppearance="@style/username"
        android:layout_marginBottom="274dp"
        android:layout_marginTop="143dp"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="177dp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_1"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_1"
        app:layout_constraintStart_toStartOf="@+id/rectangle_1"
        app:layout_constraintTop_toTopOf="@+id/rectangle_1" />

    <TextView
        android:id="@+id/password"
        android:layout_width="77dp"
        android:layout_height="24dp"
        android:layout_alignParentTop="true"
        android:gravity="top"
        android:text="@string/password"
        android:textAppearance="@style/username"
        android:layout_marginTop="218dp"
        android:layout_marginBottom="199dp"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="183dp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_1"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_1"
        app:layout_constraintStart_toStartOf="@+id/rectangle_1"
        app:layout_constraintTop_toTopOf="@+id/rectangle_1" />

    <TextView
        android:id="@+id/belum_mempu"
        android:layout_width="188dp"
        android:layout_height="16dp"
        android:layout_alignParentTop="true"
        android:gravity="top"
        android:text="@string/belum_mempu"
        android:textAppearance="@style/belum_mempu"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="417dp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_1"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_1"
        app:layout_constraintStart_toStartOf="@+id/rectangle_1"
        app:layout_constraintTop_toTopOf="@+id/rectangle_1" />

    <View
        android:id="@+id/rectangle_4"
        android:layout_width="131dp"
        android:layout_height="35dp"
        android:layout_alignParentTop="true"
        android:background="@drawable/rectangle2"
        android:layout_marginTop="310dp"
        android:layout_marginBottom="96dp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_1"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_1"
        app:layout_constraintStart_toStartOf="@+id/rectangle_1"
        app:layout_constraintTop_toTopOf="@+id/rectangle_1" />

    <TextView
        android:id="@+id/login"
        android:layout_width="51dp"
        android:layout_height="27dp"
        android:layout_alignParentTop="true"
        android:gravity="top"
        android:text="@string/login"
        android:textAppearance="@style/login"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_4"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_4"
        app:layout_constraintStart_toStartOf="@+id/rectangle_4"
        app:layout_constraintTop_toTopOf="@+id/rectangle_4" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="47dp"
        android:layout_marginBottom="324dp"
        android:layout_marginStart="108dp"
        android:layout_marginEnd="108dp"
        app:layout_constraintBottom_toBottomOf="@+id/rectangle_1"
        app:layout_constraintEnd_toEndOf="@+id/rectangle_1"
        app:layout_constraintStart_toStartOf="@+id/rectangle_1"
        app:layout_constraintTop_toTopOf="@+id/rectangle_1"
        app:srcCompat="@drawable/Temperature" />


</androidx.constraintlayout.widget.ConstraintLayout>